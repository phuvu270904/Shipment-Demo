{
  "name": "nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start -b swc",
    "start:dev": "cross-env NODE_ENV=development nest start -b swc -w",
    "start:debug": "cross-env NODE_ENV=staging nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:create-migration": "cross-env NODE_ENV=development npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "typeorm:create-migration:staging": "cross-env NODE_ENV=staging npm run typeorm -- migration:create ./src/migrations-staging/$npm_config_name",
    "typeorm:generate-migration": "cross-env NODE_ENV=development npm run typeorm -- migration:generate -d ./src/ormconfig.ts ./src/migrations/$npm_config_name",
    "typeorm:generate-migration:staging": "cross-env NODE_ENV=staging npm run typeorm -- migration:generate -d ./src/ormconfig.ts ./src/migrations-staging/$npm_config_name",
    "typeorm:run-migrations": "cross-env NODE_ENV=development npm run typeorm -- migration:run -d ./src/ormconfig.ts",
    "typeorm:run-migrations:staging": "cross-env NODE_ENV=staging npm run typeorm -- migration:run -d ./src/ormconfig.ts",
    "typeorm:revert-migrations": "cross-env NODE_ENV=development npm run typeorm -- migration:revert -d ./src/ormconfig.ts",
    "typeorm:revert-migrations:staging": "cross-env NODE_ENV=staging npm run typeorm -- migration:revert -d ./src/ormconfig.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^2.0.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.5.0",
    "firebase": "^11.9.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.1.0",
    "loadsh": "^0.0.4",
    "multer": "^2.0.1",
    "mysql2": "^3.6.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
